               <VBox.margin>
                  <Insets bottom="10.0" left="50.0" top="20.0" />
               </VBox.margin>
            </Button>
            <Label fx:id="searchPharmacy" onMouseClicked="#goSearchPharmacy" text="Search for nearby Pharmacy">
